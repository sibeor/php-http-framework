{
    "name": "sibeor/php-http-framework",
    "type": "project",
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Framework\\": "src/Framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\App\\": "tests/App/",
            "Test\\Framework\\": "tests/Framework/"
        }
    }
}
